package bs.pi.gateway.client.http;

import net.sf.json.JSONObject;

public class HttpMsgReceive {
	
	public final static int TYPE_DEVICE_CMD = 1;
	
	private boolean executeSuccess;
	private int type;
	private JSONObject data;
	public int getType() {
		return type;
	}
	public void setType(int type) {
		this.type = type;
	}
	public JSONObject getData() {
		return data;
	}
	public void setData(JSONObject data) {
		this.data = data;
	}
	public boolean getExecuteSuccess() {
		return executeSuccess;
	}
	public void setExecuteSuccess(boolean executeSuccess) {
		this.executeSuccess = executeSuccess;
	}
	
}
