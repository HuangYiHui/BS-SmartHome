package com.zigbeee;

public class Test {

	public static void main(String[] args) {
		
		class NS
		{
			private int value;

			public int getValue() {
				return value;
			}

			public void setValue(int value) {
				this.value = value;
			}
			
		}
		
		NS ns = new NS();
		ns.setValue(123);
		
		NS xs = ns;
		
		ns.setValue(321);
		
		System.out.println(xs.getValue());
	}

}
