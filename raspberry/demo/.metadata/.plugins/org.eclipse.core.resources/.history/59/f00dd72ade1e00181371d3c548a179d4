package com.zigbeee;

import java.util.Arrays;

public class Test {

	public static void main(String[] args) {
		byte[] bs = {0x00, 0x00, 0x00, 0x01};
		byte[] result = ZBCG.chanlistCfg(bs);
		for(byte b : result){
			System.out.printf("%x", b);
			System.out.print(',');
		}
		
	}
	
	public static void print(byte[] bs){
		for(byte b : bs){
			System.out.printf("%x", b);
			System.out.print(',');
		}
	}

}
