package com.zigbeee;

public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] ns = new int[3];
		ns[0] = 1;
		ns[1] = 2;
		ns[2] = 3;
		
		int[] xs = ns.clone();
		
		ns[2] = 4;
		
		System.out.println(xs[0]);
		System.out.println(xs[1]);
		System.out.println(xs[2]);
	}

}
