package bs.pi.gateway.client.http;

import java.io.FileInputStream;
import java.util.Properties;

public class HttpMsgClientCfg {
	
	private String serviceUrl;
	private String deviceID;
	private String apiKey;
	
	public HttpMsgClientCfg(){
		
	}
	
	public HttpMsgClientCfg(String cfgPath){
    	
	}
	
	public void loadCfg(String cfgPath){
		Properties properties = new Properties();
		try {
			properties.load(new FileInputStream(cfgPath));
			serviceUrl = properties.getProperty("serviceUrl");
			deviceID = properties.getProperty("deviceID");
			apiKey = properties.getProperty("apiKey");
		} catch (Exception e) {
			System.err.println(">>>HttpMsgClientCfgº”‘ÿ≈‰÷√≥ˆ¥Ì ,cfgPath:"+cfgPath);
			e.printStackTrace();
		}
	}

	public String getServiceUrl() {
		return serviceUrl;
	}
	public void setServiceUrl(String serviceUrl) {
		this.serviceUrl = serviceUrl;
	}
	public String getDeviceID() {
		return deviceID;
	}
	public void setDeviceID(String deviceID) {
		this.deviceID = deviceID;
	}
	public String getApiKey() {
		return apiKey;
	}
	public void setApiKey(String apiKey) {
		this.apiKey = apiKey;
	}
	
}
