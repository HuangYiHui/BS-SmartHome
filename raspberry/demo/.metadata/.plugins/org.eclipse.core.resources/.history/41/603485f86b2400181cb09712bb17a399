package bs.pi.gateway.main;

public class AppMsg {
	
	/*
	 * 
	 * 一般命令采用HashMap<String, Object>存放， 格式：
	 * 		CmdName : TestCmd,...
	 * 上传传感器值到http服务器的命令格式：
	 * 		Cmd : UploadDataToHttpServer
	 * 		sensorID : 123(ID号，字符串，一般为整数数字字符串)
	 * 		sensorValue : 123.456
	 */
	
	public final static String K_CMD_NAME = "CmdName";
	public final static String K_SENSOR_ID = "SensorID";
	public final static String K_SENSOR_VALUE = "SensorValue";
	public final static String K_HTTP_SUCESS = "success";
	public final static String K_HTTP_MSG = "msg";
	public final static String K_APP_ID = "AppID";
	public final static String K_CMD_PARAMS = "HttpDeviceCmdParams";
	public final static String K_PORT_CMD0 = "Cmd0";
	public final static String K_PORT_CMD1 = "Cmd1";
	public final static String K_PORT_DATA = "Data";
	
	public final static String V_CMD_NAME_UPLOAD_DATA_TO_HTTP_SERVER = "UploadDataToHttpServer";
	public final static String V_CMD_NAME_UPLOAD_DATA_TO_HTTP_SERVER_RESPONSE = "UploadDataToHttpServerResponse";
	public final static String V_CMD_NAME_GET_DEVICE_CMD_FROM_HTTP_SERVER = "GetDeviceCmdFromHttpServer";
	public final static String V_CMD_NAME_GET_DEVICE_CMD_FROM_HTTP_SERVER_RESPONSE = "GetDeviceCmdFromHttpServerResponse";
	public final static String V_CMD_NAME_HTTP_DEVICE_CMD = "HttpDeviceCmd";
	public final static String V_CMD_NAME_PORT_DATA_ARRIVE = "PortDataArrive";
	public final static String V_CMD_NAME_SEND_CMD_TO_APP = "SendCmdToApp";
	public final static String V_CMD_NAME_PORT_SEND = "PortSend";
	
	public final static String V_HTTP_TRUE = "true";
	public final static String V_HTTP_FALSE = "false";
	public final static String V_HTTP_EXCEPTION = "exception";
}

