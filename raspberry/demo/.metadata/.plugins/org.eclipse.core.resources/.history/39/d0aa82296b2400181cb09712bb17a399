package bs.pi.gateway.main;

import java.util.HashMap;

public interface ISender {

	public final static String K_SEND_RESULT = "SendResult";
	public final static String K_SEND_RESULT_SUCCESS = "Success";
	public final static String K_SEND_RESULT_FAIL = "Fail";
	
	public final static String V_SEND_NAME_HTTP_SNEDER = "HttpSender";
	public final static String V_SEND_NAME_PORT_SNEDER = "PortSender";
	public final static String V_SEND_NAME_ZIGBEE_SNEDER = "ZigbeeSender";
	
	public String getName();
	public HashMap<String, Object> send(HashMap<String, Object> msg);  
}
