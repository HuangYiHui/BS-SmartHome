package com.zigbeee;

public class ZBPacketSend {

	private byte[] dstAddr;
	private byte dstEndpoint;
	private byte srcEndpoint;
	private byte[] clusterID;
	private byte transID;
	private byte options;
	private byte radius;
	private byte len;
	private byte[] data;
	
	ZBPacketSend(){
		dstAddr = new byte[2];
		clusterID = new byte[2];
	}
	
}
